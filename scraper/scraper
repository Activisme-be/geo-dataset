#!/usr/bin/env php
<?php
// application.php

// Debugging code
ini_set('display_errors', 1);
ini_set('display_startup_errors', 1);
error_reporting(E_ALL);
//> End debugging code

require __DIR__.'/vendor/autoload.php';

use ActivismeBe\Scraper\Commands\CitiesCommand;
use Symfony\Component\Console\Application;

$shell = new Application('Scraper', 'v1.0.0');
$shell->add(new CitiesCommand());
$shell->run();